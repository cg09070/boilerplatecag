    "file-loader": "^6.2.0",
    "glslify-loader": "^2.0.0",
    "gsap": "^3.12.7",
    "image-minimizer-webpack-plugin": "^4.1.3",
    "imagemin": "^9.0.0",
    "include-media": "^2.0.0",
    "mini-css-extract-plugin": "^2.9.2",
    "nodemon": "^3.1.9",
    "normalize-wheel": "^1.0.1",
    "postcss-loader": "^8.1.1",
    "prefix": "^1.0.0",
    "raw-loader": "^4.0.2",
    "sass": "^1.84.0",
    "sass-loader": "^16.0.4",
    "webpack": "^5.97.1",
